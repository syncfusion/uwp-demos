<Page
    x:Class="Syncfusion.SampleBrowser.UWP.PdfViewer.PdfViewer.UWP_10710"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:sfpdf="using:Syncfusion.Windows.PdfViewer"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid Loaded="Grid_Loaded">
        <Grid.RowDefinitions>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <Button x:Name="Unload" Grid.Row="0" Content="Unload" Click="Unload_Click"></Button>
        <sfpdf:SfPdfViewerControl x:Name="pdfViewer" Grid.Row="1"  HyperlinkPointerPressed="pdfViewerControl_HyperlinkPointerPressed"/>
    </Grid>
</Page>
